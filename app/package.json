{
  "name": "paramax_repo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/app",
    "apps/backoffice",
    "apps/frontend"
  ],
  "type": "module",
  "scripts": {
    "storybook": "npm run storybook --workspace @paramax_repo/app",
    "prettier:apps": "npx prettier --check 'apps/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "test:unit:apps": "vitest run --project app --project backoffice --project frontend",
    "test:unit:all": "npm run test:unit:functions && npm run test:unit:shared && npm run test:unit:apps",
    "lint": "npm run lint:app && npm run lint:backoffice && npm run lint:functions && npm run lint:shared",
    "prettier:functions": "npx prettier --check 'functions/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "build:functions": "npm --prefix ./functions run build",
    "build:watch:functions": "npm --prefix ./functions run build:watch",
    "serve:functions": "npm --prefix ./functions run serve",
    "lint:functions": "npm --prefix ./functions run lint",
    "test:unit:functions": "npm --prefix ./functions run test:unit",
    "build:app": "npm run build --workspace @paramax_repo/app",
    "dev:app": "npm run dev --workspace @paramax_repo/app",
    "stage:app": "npm run stage --workspace @paramax_repo/app",
    "preview:app": "npm run preview --workspace @paramax_repo/app",
    "install:app": "npm install --workspace @paramax_repo/app",
    "lint:app": "npm run lint --workspace @paramax_repo/app",
    "lint:quiet:app": "npm run lint:quiet --workspace @paramax_repo/app",
    "lint:fix:app": "npm run lint:fix --workspace @paramax_repo/app",
    "test:unit:app": "npm run test:unit --workspace @paramax_repo/app --mock",
    "test:unit-dev:app": "npm run test:unit-dev --workspace @paramax_repo/app --mock",
    "test:e2e:mock:app": "npm run test:e2e --workspace @paramax_repo/app --mock",
    "test:e2e:default:app": "npm run test:e2e --workspace @paramax_repo/app",
    "build:backoffice": "npm run build --workspace @paramax_repo/backoffice",
    "dev:backoffice": "npm run dev --workspace @paramax_repo/backoffice",
    "stage:backoffice": "npm run stage --workspace @paramax_repo/backoffice",
    "preview:backoffice": "npm run preview --workspace @paramax_repo/backoffice",
    "install:backoffice": "npm install --workspace @paramax_repo/backoffice",
    "lint:backoffice": "npm run lint --workspace @paramax_repo/backoffice",
    "lint:quiet:backoffice": "npm run lint:quiet --workspace @paramax_repo/backoffice",
    "lint:fix:backoffice": "npm run lint:fix --workspace @paramax_repo/backoffice",
    "test:unit:backoffice": "npm run test:unit --workspace @paramax_repo/backoffice",
    "test:unit-dev:backoffice": "npm run test:unit-dev --workspace @paramax_repo/backoffice",
    "test:unit:frontend": "npm run test:unit --workspace @paramax_repo/frontend",
    "test:unit-dev:frontend": "npm run test:unit-dev --workspace @paramax_repo/frontend ",
    "lint:shared": "npm --prefix ./shared run lint",
    "lint:quiet:shared": "npm --prefix ./shared run lint:quiet",
    "lint:fix:shared": "npm --prefix ./shared run lint:fix",
    "test:unit:shared": "npm --prefix ./shared run test:unit"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.0.18",
    "@material-tailwind/react": "^2.1.8",
    "@reduxjs/toolkit": "^1.9.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/ffprobe-static": "^2.0.3",
    "@types/react-beautiful-dnd": "^13.1.7",
    "canvas": "^2.11.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.11.1",
    "fluent-ffmpeg": "^2.1.2",
    "fluid-player": "^3.29.0",
    "get-audio-duration": "^4.0.1",
    "hls.js": "^1.4.13",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.0",
    "redux": "^4.2.1",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fs": "^0.0.1-security",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vitest-canvas-mock": "^0.3.3"
  }
}
